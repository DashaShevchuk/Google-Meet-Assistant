{
  "manifest_version": 3,
  "name": "Google Meet Assistant",
  "version": "1.0",
  "description": "Beaver, which helps you collect information about a Google Meet conference",
  "key": "",
  "permissions": ["storage", "alarms", "activeTab", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "content_scripts": [
    {
      "matches": ["*://meet.google.com/*"],
      "js": ["content.js"]
    }
  ]
}
